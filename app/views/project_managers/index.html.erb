<p id="notice"><%= notice %></p>

<h1>Listing Project Managers</h1>

<table>
  <thead>
    <tr>
      <th>Project</th>
      <th>Person</th>
      <th>Primary</th>
      <th>Active date</th>
      <th>Inactive date</th>
      <th>Created by</th>
      <th>Created at</th>
      <th>Updated by</th>
      <th>Updated at</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @project_managers.each do |project_manager| %>
      <tr>
        <td><%= project_manager.project_id %></td>
        <td><%= project_manager.person_id %></td>
        <td><%= project_manager.primary %></td>
        <td><%= project_manager.active_date %></td>
        <td><%= project_manager.inactive_date %></td>
        <td><%= project_manager.created_by %></td>
        <td><%= project_manager.created_at %></td>
        <td><%= project_manager.updated_by %></td>
        <td><%= project_manager.updated_at %></td>
        <td><%= link_to 'Show', project_manager %></td>
        <td><%= link_to 'Edit', edit_project_manager_path(project_manager) %></td>
        <td><%= link_to 'Destroy', project_manager, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Project manager', new_project_manager_path %>
