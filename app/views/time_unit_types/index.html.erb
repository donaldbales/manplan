<p id="notice"><%= notice %></p>

<h1>Listing Time Unit Types</h1>

<table>
  <thead>
    <tr>
      <th>Code</th>
      <th>Description</th>
      <th>Url</th>
      <th>Equivalent hours</th>
      <th>Active date</th>
      <th>Inactive date</th>
      <th>Created by</th>
      <th>Created at</th>
      <th>Updated by</th>
      <th>Updated at</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @time_unit_types.each do |time_unit_type| %>
      <tr>
        <td><%= time_unit_type.code %></td>
        <td><%= time_unit_type.description %></td>
        <td><%= time_unit_type.url %></td>
        <td><%= time_unit_type.equivalent_hours %></td>
        <td><%= time_unit_type.active_date %></td>
        <td><%= time_unit_type.inactive_date %></td>
        <td><%= time_unit_type.created_by %></td>
        <td><%= time_unit_type.created_at %></td>
        <td><%= time_unit_type.updated_by %></td>
        <td><%= time_unit_type.updated_at %></td>
        <td><%= link_to 'Show', time_unit_type %></td>
        <td><%= link_to 'Edit', edit_time_unit_type_path(time_unit_type) %></td>
        <td><%= link_to 'Destroy', time_unit_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

